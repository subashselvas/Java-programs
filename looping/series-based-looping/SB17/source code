import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int num=sc.nextInt();
	    int temp=num;
	    int sum=0;
	    while(temp>0){
	        int di=temp%10;//153//3
	        
	        sum=sum+di*di*di;//27
	        temp/=10;//15
	    }
	    if(sum==num){
	        System.out.println("Armstrong Number");
	    }
	    else{
	        System.out.println("Not Armstrong Number");
	    }
		
	}
}
 
 